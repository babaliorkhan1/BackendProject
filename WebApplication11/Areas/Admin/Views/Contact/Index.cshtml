@model Contact

<div class="container">
    <table class="table table-sm">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Adress</th>
                <th scope="col">Update</th>
                <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>



            <tr>
                <th scope="row">@Model.Id</th>
                <th scope="row">@Model.Adress</th>
                <td><a asp-controller="contact" asp-action="update" asp-route-id="@Model.Id"><i class="fas fa-edit text text-primary"></i></a></td>
                <td><a onclick="if(!confirm('Are Your Sure?')){return false;}" asp-controller="contact" asp-action="delete" asp-route-id="@Model.Id" class=" delete-item"><i class="fas fa-trash text text-danger"></i></a></td>
            </tr>


        </tbody>
    </table>
    <div>
        <a asp-controller="contact" asp-action="create" style="display: block; text-decoration: none; color: white; width: 100%; height: 100%"> <div style="width:100%; " class="btn btn-outline-success">Contact yarat </div></a>
    </div>

</div>
